{
  "name": "@sullivan/redux-config",
  "version": "0.6.0",
  "description": "Reduces redux boilerplate with a configurable structure",
  "main": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "lint": "npx eslint",
    "lint:src": "yarn lint src",
    "test": "npx jest --config .jest/config.js",
    "test:async": "node examples/actions/asyncSuccess",
    "test:creator": "node examples/actions/actionCreator",
    "prepare": "npx babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icarus-sullivan/redux-config.git"
  },
  "keywords": [
    "redux",
    "config",
    "boilerplate",
    "simple"
  ],
  "author": "Chris Sullivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icarus-sullivan/redux-config/issues"
  },
  "homepage": "https://github.com/icarus-sullivan/redux-config#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "redux": "^4.0.4"
  }
}
